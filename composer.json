{
  "name": "vulcandigital/silverstripe-gravatar",
  "description": "A very simple Gravatar integration",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Reece Alexander",
      "email": "reece@vulcandigital.co.nz"
    }
  ],
  "require": {
    "silverstripe/recipe-core": "^1"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Vulcan\\Gravatar\\": "src/",
      "Vulcan\\Gravatar\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
